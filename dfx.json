{
  "canisters": {
    "backend": {
      "main": "backend/app.mo",
      "type": "motoko",
      "args": "--enhanced-orthogonal-persistence",
      "init_arg": "(variant { testnet })"
    },
    "frontend": {
      "dependencies": ["backend"],
      "frontend": {
        "entrypoint": "frontend/index.html"
      },
      "source": ["frontend/dist"],
      "type": "assets"
    }
  },
  "output_env_file": ".env",
    "defaults": {
    "bitcoin": {
      "enabled": true,
      "nodes": [
        "127.0.0.1:18444"
      ],
      "log_level": "info"
    },
    "build": {
      "packtool": "mops sources",
      "args": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943"
    }
  }
}
